  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug frontend",
      "cargo": {
        "args": [
          "build",
          "--bin=lidar2d-frontend",
          "--package=tether-lidar2d-consolidation"
        ],
        "filter": {
          "name": "lidar2d-frontend",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'tether-lidar2d-consolidation-rs'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=tether-lidar2d-consolidation-rs",
          "--package=tether-lidar2d-consolidation-rs"
        ],
        "filter": {
          "name": "tether-lidar2d-consolidation-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}